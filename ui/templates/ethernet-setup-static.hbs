[connection]
id=my-ethernet
type=ethernet
interface-name={{adapter_name}}
permissions=
secondaries=

[ethernet]
mac-address-blacklist=

[ipv4]
address1={{ip_address}}/24
dns={{#each dns}}{{#if @last}}{{this}}{{else}}{{this}}, {{/if}}{{/each}}
dns-search=
method=manual

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto
